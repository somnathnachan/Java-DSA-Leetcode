class Solution {
    public int countOdds(int low, int high) {
        int num;

        if(low % 2 != 0)
        {
            //odd
            num = low;
        }
        else
        {
            //even
            num = low + 1;
        }

        int count = 0;

        while(num <= high)
        {
            count++;
            num += 2;
        }

        return count;
    }
}
